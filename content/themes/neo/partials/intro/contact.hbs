<div class="intro" data-align="center">
	<div class="intro-wrap">
		<div class="intro-wrap-inner">
			<div class="welcome-text">
				<h1>{{{title}}}</h1>				
			</div>
		</div>
	</div>
	{{#if image}}
	<div class="intro-background" style="background-image: url({{image}})"
		data-bottom-top="transform: translate3d(0px,-200px, 0px);"
		data-top-bottom="transform: translate3d(0px,200px, 0px);"
		data-center="transform: translate3d(0px,0px, 0px);">
	</div>
	{{else}}
	<div class="gmap-container">
		<div class="gmap" id="gmap"
			data-latitude="10.839776" {{!Change Your map latitude here}}
			data-longitude="106.647159" {{!Change Your map longitude here}}
		>
		</div>
	</div>
	{{/if}}
</div>